{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --no-open --port 3001",
    "start:cn": "docusaurus start --no-open --port 3001 --locale zh-CN",
    "copy": "node scripts/contributing.js && node scripts/syncExampleDocs.js",
    "build": "npm run copy && docusaurus build",
    "build:cn": "npm run copy && CHINESE_DOCS=1 docusaurus build --locale zh-CN",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "2.3.1",
    "@docusaurus/preset-classic": "2.3.1",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.3.1",
    "@tsconfig/docusaurus": "^1.0.6",
    "typescript": "^4.6.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
